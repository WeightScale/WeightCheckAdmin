    <style name="CustomTemperatureProgressBar" parent="android:Widget.ProgressBar.Horizontal">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/temperature</item>
        <item name="android:max">120</item>
        <item name="android:secondaryProgress">120</item>
    </style>

    <style name="CustomBatteryProgressBar" parent="android:Widget.ProgressBar.Horizontal">
        <item name="android:layout_width">@dimen/width_battery</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/battery</item>
        <item name="android:background">@drawable/battery_low</item>
        <item name="android:max">100</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">3dp</item>
    </style>

